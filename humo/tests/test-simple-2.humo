#class Rectangle
{
	#method getArea
	{
		[line]{#var multiplier:p1{#this.width}}
		[line]{#var multiplier:p2{#this.length}}
		[line]{#var instance:name{mult1}}
		[line]{#new Multiplier}
		[line]{#call mult1.multiply}
		[line]{#var area:result{<process-result>multiply:result}}
	}

	#this.width
	{
		#parameter rectangle:width
	}

	#this.length
	{
		#parameter rectangle:length
	}
}

#class Multiplier
{
	#method multiply
	{
		[line]{#var multiply:first{#this.firstValue}}
		[line]{#var multiply:second{#this.secondValue}}
		[line]{#var multiply:combined{$multiply:first:mult:$multiply:second}}
		[line]{#var multiply:result{#call $multiply:combined}}
	}

	#this.firstValue
	{
		#parameter multiplier:p1
	}

	#this.secondValue
	{
		#parameter multiplier:p2
	}
}


